# Backend service port
PORT=8888

# Base URL for serving assets (including protocol and port if necessary)
# In production, this should be your public API URL
BASE_URL=http://localhost:8888

# Output directory for generated files
OUTPUT_DIR=outputs

# Enable CORS for cross-domain API access 
ENABLE_CORS=true

# Maximum queue size for concurrent jobs
MAX_QUEUE_SIZE=10

# Optional path to Chrome executable
# CHROME_PATH=/usr/bin/google-chrome

# Logging level (debug, info, warn, error)
LOG_LEVEL=info

# Database Configuration
DB_PATH=./data/generations.db

# Sentry Configuration
SENTRY_DSN=https://your-sentry-project-key@sentry.io/your-project-id
SENTRY_ENVIRONMENT=development
SENTRY_RELEASE=1.0.0
SENTRY_DEBUG=false

# Admin Authentication
# Set this to restrict access to the history API endpoints
# If not set, admin authentication is disabled
# ADMIN_TOKEN=your-secure-admin-token

# Chrome Configuration
CHROME_PATH= 