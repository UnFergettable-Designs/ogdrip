# Backend service port
PORT=8888

# Base URL for serving assets (including protocol and port if necessary)
# In production, this should be your public API URL
BASE_URL=https://api.yourdomain.com

# Output directory for generated files
OUTPUT_DIR=outputs

# Enable CORS for cross-domain API access 
ENABLE_CORS=true

# Maximum queue size for concurrent jobs
MAX_QUEUE_SIZE=10

# Optional path to Chrome executable
# CHROME_PATH=/usr/bin/google-chrome

# Logging level (debug, info, warn, error)
LOG_LEVEL=info 